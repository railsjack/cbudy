{
  "expo": {
    "name": "CBudyApp",
    "description": "This is an app for CBudy",
    "slug": "cbudy-app",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/images/app-icon.png",
    "splash": {
      "image": "./src/images/launch.png",
      "mdpi": "./src/images/launch.png",
      "hdpi": "./src/images/launch.png",
      "xhdpi": "./src/images/launch.png",
      "xxhdpi": "./src/images/launch.png",
      "xxxhdpi": "./src/images/launch.png",
      "resizeMode": "cover",
      "backgroundColor": "#0cb464"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.cbudy"
    },
    "android": {
      "package": "com.cbudy",
      "googleServicesFile": "./google-services.json"
    }
  }
}
